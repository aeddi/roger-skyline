APP_NAME = Gogs: Go Git Service
RUN_USER = git
RUN_MODE = prod

[database]
DB_TYPE  = postgres
HOST     = 127.0.0.1:5432
NAME     = {{ gogs_db_name }}
USER     = {{ gogs_db_user }}
PASSWD   = {{ gogs_db_password }}
SSL_MODE = disable
PATH     = data/gogs.db

[repository]
ROOT = /home/git/gogs-repositories

[server]
DOMAIN       = git.slash16.local
HTTP_PORT    = 3000
ROOT_URL     = https://10.17.1.154:4432/
DISABLE_SSH  = true
SSH_PORT     = 22
OFFLINE_MODE = true

[mailer]
ENABLED = true
HOST = mail.slash16.local:25
FROM = "Gogs" <no-reply@slash16.local>
USER = no-reply@slash16.local
SKIP_VERIFY = true

[service]
REGISTER_EMAIL_CONFIRM = true
ENABLE_NOTIFY_MAIL     = true
DISABLE_REGISTRATION   = true
ENABLE_CAPTCHA         = true
REQUIRE_SIGNIN_VIEW    = false

[picture]
DISABLE_GRAVATAR        = false
ENABLE_FEDERATED_AVATAR = false

[session]
PROVIDER = file

[log]
MODE      = file
LEVEL     = Info
ROOT_PATH = /home/git/goworkspace/src/github.com/gogits/gogs/log

[security]
INSTALL_LOCK = true
SECRET_KEY   = n3k6Xn5lBUGsxdA
